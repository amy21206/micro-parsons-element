{
  "name": "micro-parsons",
  "version": "0.1.6",
  "description": "An implentation of Micro Parsons input",
  "private": true,
  "watch": {
    "build": {
      "patterns": ["src"],
      "extensions": "ts,css",
      "quiet":false
    }
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "if [ -d ./bin ]; then rm -r ./bin; fi && tsc && mkdir ./bin/style && cp -r ./src/style/* ./bin/style/ && rollup -c",
    "watch": "npm-watch"
  },
  "author": "Zihan Wu",
  "license": "MIT",
  "dependencies": {
    "highlight.js": "^11.7.0",
    "npm-watch": "^0.11.0",
    "sortablejs": "^1.14.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^17.1.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^11.2.1",
    "@rollup/plugin-terser": "^0.2.1",
    "@rollup/plugin-typescript": "^8.2.1",
    "@types/quill": "^2.0.9",
    "@types/sortablejs": "^1.10.7",
    "http-server": "^0.12.3",
    "rollup": "^2.52.8",
    "rollup-plugin-filesize": "^9.1.1",
    "rollup-plugin-import-css": "^2.0.1",
    "rollup-plugin-node-externals": "^5.1.0",
    "rollup-plugin-node-polyfills": "^0.2.1",
    "rollup-plugin-terser": "^7.0.2",
    "tslib": "^2.3.0",
    "typescript": "^4.3.5"
  },
  "files": [
    "micro-parsons/*"
  ],
  "main": "index.js",
  "module": "index.js"
}
